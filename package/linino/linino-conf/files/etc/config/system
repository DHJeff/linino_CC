config system
	option hostname	arduino
	option timezone	UTC

config timeserver ntp
	list server	0.openwrt.pool.ntp.org
	list server	1.openwrt.pool.ntp.org
	list server	2.openwrt.pool.ntp.org
	list server	3.openwrt.pool.ntp.org
	option enable_server 0

config led 'led_wlan'
	option name 'WLAN'
	option sysfs 'wlan'
	option trigger 'netdev'
	option dev 'wlan0'
	option mode 'tx rx'
	option default '0'

config led 'led_usb'
	option name 'USB'
	option sysfs 'usb'
	option trigger 'usbdev'
	option dev '1-1'
	option interval '50'
	option default '0'

config led 'led_lan'
	option name 'LAN'
	option sysfs 'ath9k-phy0'
	option trigger 'netdev'
	option dev 'eth0'
	option mode 'link tx rx'
	option default '0'
